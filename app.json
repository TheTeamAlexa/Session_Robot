{
  "name": "Pyrogram And Telethon String Session Generator",
  "description": "Generate Pyrogram And Telethon String Session Easily",
  "logo": "https://telegra.ph/file/27bfcfbb9d72fb3f3a1ca.jpg",
  "keywords": ["telegram", "bot", "python", "pyrogram"],
  "buildpacks": [{"url": "heroku/python"}],
  "formation": {
    "worker": {
      "quantity": 1,
    }
  },
  "addons": [
    {
      "options": {
        "version": "12"
      },
      "plan": "heroku-postgresql"
    }
  ],
  "repository": "https://github.com/jankarikiduniya/Session_Robot",
  "env": {
    "ENVIRONMENT": {
      "description": "Don't change ANYTHING.",
      "required": true,
      "value": "ANYTHING"
    },
    "API_ID": {
      "description": "Get this value from my.telegram.org.",
      "required": true,
      "value": "your_api_id_here"
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org.",
      "required": true,
      "value": "your_api_hash_here"
    },
    "BOT_TOKEN": {
      "description": "Obtain a Telegram bot token by contacting @BotFather",
      "required": true,
      "value": "your_bot_token_here"
    },
    "MUST_JOIN": {
      "description": "Add username/id of a channel/group",
      "required": false,
      "value": "https://t.me/Alexa_Help"
    }
  }
}
